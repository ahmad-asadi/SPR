D =[0.0 1
    0.1 2
    0.1 9
    0.3 2
    0.4 1
    0.4 8];
h = 0.1 ;
N = size(D,1) ;
[X1 , X2] = meshgrid(-5:0.01:5) ;
Y = zeros(size(X1,2)*size(X2,1)) ;
for i = 1 : size(X1,2)
   for j = 1 : size(X2,1) 
       x1 = X1(i,2);
       x2 = X2(i,1) ;
       x = [x1 ; x2] ;
       t = 0 ;
       for k = 1 : size(D,1)
           XK = D(k,:).' ;
           t = t + exp(-0.5 * (X-XK).'*(X-XK)) ;
       end
       Y(i,j) = t /(N*pi*(h^2)) ;     
   end
end